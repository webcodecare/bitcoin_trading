# CryptoStrategy Pro Environment Variables
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://your_database_url_here

# Stripe Payment Configuration
VITE_STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key_here
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here

# Session Security
SESSION_SECRET=your_session_secret_here

# External Market Data APIs (Optional - platform works with demo data)
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_SECRET_KEY=your_binance_secret_key_here

# AI Features (Optional)
OPENAI_API_KEY=your_openai_api_key_here

# TradingView Webhook (Optional)
TRADINGVIEW_WEBHOOK_SECRET=default_secret

# Development Settings
NODE_ENV=development
REPLIT_DOMAINS=your_replit_domain_here